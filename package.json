{
  "name": "wiserquestions",
  "version": "0.0.0",
  "description": "",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "test": "./node_modules/.bin/mocha --require ts-node/register --extensions ts,tsx src 'src/**/*.{ts,tsx}'",
    "test:watch": "npm run test -- --watch --watch-files src 'src/**/*.{ts,tsx}'",
    "coverage": "node node_modules/nyc/bin/nyc.js --exclude '**/*.spec.ts' --exclude 'src/config.ts' --extension .ts --reporter lcov --reporter text npm run test",
    "start": "./node_modules/.bin/nodemon -q --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --delay 250ms --exec 'npm run build && node ./lib/wiserquestions' src/index.ts",
    "build": "npm run clean && node ./node_modules/.bin/webpack --mode production && chmod +x ./dist/wiserquestions",
    "tsc": "npm run clean && node node_modules/typescript/bin/tsc --p tsconfig.json",
    "clean": "rm -rf lib",
    "lint": "./node_modules/.bin/eslint . --ext ts",
    "docs": "node node_modules/typedoc/bin/typedoc --out docs/ src/ && echo '*\n!.gitignore' > docs/.gitignore"
  },
  "license": "MIT",
  "dependencies": {
    "csvtojson": "^2.0.10",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "tsnode-di": "0.0.3"
  },
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/cors": "^2.8.13",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "chai": "^4.3.7",
    "eslint": "^8.36.0",
    "mocha": "^10.2.0",
    "nodemon": "^1.12.1",
    "npm-run-parallel": "^0.6.0",
    "nyc": "^15.1.0",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.28",
    "typescript": "^5.0.2",
    "webpack": "^5.76.3",
    "webpack-cli": "^5.0.1"
  }
}
